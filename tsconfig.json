{
  "include": ["src/**/*"],
  "exclude": [],
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "node16",
    "moduleResolution": "node16",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
  }
}
